<% if !logged_in? %>
  <%= link_to "Services", categories_path %>
  <%= link_to "Log In", login_path %>
  <%= link_to "Become A Tasker" %>
<% else %>
  <%= link_to "Services", categories_path %>
  <%= link_to "Welcome, #{current_user.username}", user_path(current_user.id) %>
  <%= link_to "Become A Tasker" %>
  <%= link_to "Log Out", logout_path %>
<% end %>
