<div class="title">
  <h2><%= name %></h2>
</div>
<div>
  <%= f.label "Address" %>
  <br>
  <%= f.text_field :response, multiple:true %>
  <br>
  <%= f.label "Number of Bathrooms" %>
  <br>
  <%= f.number_field :response, multiple:true %>
  <br>
  <br>
  <%= f.label "Type of Cleaning " %>
  <br>
  <% @cleaning_type.each do |v| %>
    <%= f.check_box :response, {multiple: true}, v, false %>
    <%= f.label v %>
  <% end %>
</div>
