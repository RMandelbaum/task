<div class="title">
  <h2><%= name %></h2>
</div>
<div>
  <%= f.label "Vehicle Size" %>
  <br>
  <% @vehicle.each do |v| %>
  <%= f.check_box :response, {multiple: true}, v, false %>
  <%= f.label v %>
  <% end %>
  <br>
  <br>
  <%= f.label "Address" %>
  <br>
  <%= f.text_area :response, multiple: true %>
  <br>
  <%= f.check_box :response, {multiple: true}, true, false %>
  <%= f.label "Receipt" %>

</div>
